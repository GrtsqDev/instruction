{
    "semi": true,
    "tabWidth": 4,
    "printWidth": 100,
    "singleQuote": false,
    "trailingComma": "all",
    "jsxSingleQuote": false,
    "bracketSpacing": true,
    "arrowParens": "always",
    "bracketSameLine": true,
    "useTabs": false,
    "endOfLine": "lf",
    "htmlWhitespaceSensitivity": "css",
    "proseWrap": "preserve",
    "quoteProps": "as-needed"
}
